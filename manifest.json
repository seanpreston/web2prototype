{
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery-2.1.3.min.js",
        "content.js"
      ]
    }
  ],
  "manifest_version": 2,
  "name": "Chrome Extension",
  "version": "0.1"
}
